generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id     Int    @id
  locale String
  Auth   Auth[]
}

model Auth {
  user      User   @relation(fields: [userId], references: [id])
  userId    Int    @id
  token     String
  timestamp Int
}
